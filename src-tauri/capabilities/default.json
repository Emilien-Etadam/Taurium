{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for all webviews",
  "webviews": ["sidebar", "settings"],
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:webview:default",
    "core:webview:allow-set-webview-size",
    "core:webview:allow-set-webview-position",
    "opener:default"
  ]
}
